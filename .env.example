# Supabase Configuration
# Copy this file to .env.local and fill in your actual Supabase project values

# Supabase Project URL - Get this from your Supabase project dashboard
# Format: https://your-project-id.supabase.co
SUPABASE_URL=https://your-project-id.supabase.co

# Supabase Anon Key - Public key for client-side operations
# This key can be safely exposed in client applications
SUPABASE_ANON_KEY=your-supabase-anon-key

# Environment Configuration
# Possible values: development, staging, production
NODE_ENV=development

# Application Configuration
# Port for the NestJS application (if running as HTTP server)
PORT=3000

# CLI Configuration
# Default provider for authentication
DEFAULT_AUTH_PROVIDER=google

# Development Configuration
# Enable debug logging
DEBUG=false

# Enable development mode features
DEV_MODE=true

# Redirect URLs for OAuth providers
OAUTH_REDIRECT_URI=/auth/callback

# Monitoring and Logging
# Log level: error, warn, info, debug
LOG_LEVEL=debug